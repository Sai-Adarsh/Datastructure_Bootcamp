#include<stdio.h>
#include<stdlib.h>

struct Node* front = NULL;
struct Node* rear = NULL;

struct Node
{
    int data;
    struct Node* next;
};

struct Node* enqueue(struct Node* front, int x)
{
    struct Node* temp = (struct Node*)malloc(sizeof(struct Node));
    temp->data = x;
    temp->next = NULL;
    if(rear==NULL)
    {
        front=rear=temp;
    }
    else
    {
        rear->next = temp;
        rear = temp;
    }
    return front;
}

void dequeue()
{
    struct Node* temp;
    if(front==NULL)
    {
        printf("Empty\n");
        return;
    }
    else
    {
        temp = front;
        front = front->next;
        free(temp);
    }
}

void print(struct Node* front)
{
    while(front!=NULL)
    {
        printf("%d\t",front->data);
        front = front->next;
    }
}

int main()
{
    front = enqueue(front,1);
    front = enqueue(front,2);
    front = enqueue(front,3);
    front = enqueue(front,4);
    dequeue();
    print(front);
}
