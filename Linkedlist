#include<bits/stdc++.h>
using namespace std;

struct Node* head=NULL;
struct Node* tail = NULL;

struct Node
{
    int data;
    Node* next;
}; 

Node* addNode(Node* head,int val)
{
    Node* temp = new Node;
    temp->data = val;
    temp->next = NULL;
    
    if(head == NULL)
    {
        head = tail =  temp;
    }
    else
    {
        tail->next = temp;
        tail = temp;
    }
    return head;
}

void print(Node* temp)
{
    while(temp!=NULL)
    {
        cout<<temp->data<<"\n";
        temp = temp->next;
    }
}

int main()
{
    head = addNode(head,1);
    head = addNode(head,2);
    head = addNode(head,3);
    print(head);
}
