#include<stdio.h>
#include<stdlib.h>

struct Node* head=NULL;
struct Node* tail = NULL;

struct Node
{
    int data;
    struct Node* next;
}; 

struct Node* addNode(struct Node* head,int val)
{
    struct Node* temp = (struct Node*)malloc(sizeof(struct Node));
    temp->data = val;
    temp->next = NULL;
    
    if(head == NULL)
    {
        head = tail =  temp;
    }
    else
    {
        tail->next = temp;
        tail = temp;
    }
    return head;
}

void print(struct Node* temp)
{
    while(temp!=NULL)
    {
        printf("%d\n",temp->data);
        temp = temp->next;
    }
}

int main()
{
    head = addNode(head,1);
    head = addNode(head,2);
    head = addNode(head,3);
    print(head);
}
